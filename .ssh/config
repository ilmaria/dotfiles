Host ilmari.xyz
    ForwardAgent yes

Host ilmari-server
    HostName cloud.autio.me
    ForwardAgent yes
    Port 2345
    User ilmari

Host *
  AddKeysToAgent yes
  UseKeychain yes
  IdentityFile ~/.ssh/id_rsa
