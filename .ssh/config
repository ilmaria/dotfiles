Host *
    AddKeysToAgent yes
    UseKeychain yes
    IdentityFile ~/.ssh/id_rsa
    ForwardAgent yes

Host *.kauppalehti.biz
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  User ec2-user
  IdentityFile ~/.ssh/id_rsa
  UserKnownHostsFile /dev/null
  User ec2-user
  IdentityFile ~/.ssh/id_rsa
  LogLevel QUIET
