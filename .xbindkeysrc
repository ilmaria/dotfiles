# Copy (Ctrl+c)
"xvkbd -no-jump-pointer -xsendevent -text '\Cc'"
F3

# paste (ctrl+v)
"xvkbd -no-jump-pointer -xsendevent -text '\Cv'"
F4

# new browser tab (ctrlt+t)
"xvkbd -no-jump-pointer -xsendevent -text '\Ct'"
F9

# Previous browser tab (Ctrl+Shift+Tab)
"xvkbd -no-jump-pointer -xsendevent -text '\C\[ISO_Left_Tab]'"
F10

# Next browser tab (Ctrl+Tab)
"xvkbd -no-jump-pointer -xsendevent -text '\C\t'"
F11

# Close browser tab (Ctrl+w)
"xvkbd -no-jump-pointer -xsendevent -text '\Cw'"
F12
